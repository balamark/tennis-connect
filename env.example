# Tennis Connect Environment Configuration
# Copy this file to .env and customize as needed

# Application Environment (development/production)
APP_ENV=development

# Database Configuration (only change if you need different values)
DB_HOST=db
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=postgres
DB_NAME=tennis_connect
DB_SSLMODE=disable

# JWT Configuration (CHANGE in production!)
JWT_SECRET=tennis-connect-dev-secret-change-in-production
JWT_EXPIRATION=60

# Server Configuration
SERVER_PORT=8080

# Frontend Configuration
REACT_APP_API_URL=http://localhost:8080/api
REACT_APP_ENV=development

# Postgres Database Configuration (for Docker)
POSTGRES_DB=tennis_connect_dev
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres

# Server Configuration
SERVER_HOST= 