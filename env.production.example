# Production Environment Configuration
# Copy this to .env when deploying to production

# Application Environment
APP_ENV=production

# Database Configuration (CHANGE THESE IN PRODUCTION!)
DB_HOST=db
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=your-secure-production-password
DB_NAME=tennis_connect
DB_SSLMODE=require

# JWT Configuration (REQUIRED - CHANGE THIS!)
JWT_SECRET=your-super-secure-production-jwt-secret-key-at-least-32-characters
JWT_EXPIRATION=30

# Server Configuration
PORT=8080

# Frontend Configuration
REACT_APP_API_URL=https://your-domain.com/api
REACT_APP_ENV=production 